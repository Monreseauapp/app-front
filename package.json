{
  "name": "app-front",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "build": "expo export --platform web --output-dir web",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format:check": "prettier --check \"app/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "test": "jest --watchAll=false",
    "test:unit": "jest --testPathPatterns=__tests__/unit",
    "test:components": "jest --testPathPatterns=__tests__/components",
    "test:snapshots": "jest --testPathPatterns=__tests__/snapshots",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:memory-leaks": "jest --testPathPatterns=__tests__/memory --detectOpenHandles --forceExit",
    "e2e:android:build": "detox build --configuration android.emu.debug",
    "e2e:android:test": "detox test --configuration android.emu.debug",
    "e2e:ios:build": "detox build --configuration ios.sim.debug",
    "e2e:ios:test": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/checkbox": "^0.5.20",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@stripe/react-stripe-js": "^3.8.0",
    "@stripe/stripe-js": "^7.6.1",
    "@stripe/stripe-react-native": "^0.50.0",
    "axios": "^1.10.0",
    "expo": "^53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-contacts": "^14.2.5",
    "expo-document-picker": "^13.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.8",
    "expo-web-browser": "~14.2.0",
    "prettier": "^3.6.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-signature-canvas": "^4.7.4",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-testing-library": "^7.6.3",
    "jest": "~29.7.0",
    "jest-expo": "^53.0.9",
    "react-native-svg": "15.11.2",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "overrides": {
    "pretty-format": "29.7.0"
  },
  "private": true
}
